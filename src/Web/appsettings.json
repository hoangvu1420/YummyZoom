{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AZURE_KEY_VAULT_ENDPOINT": "",
  "AllowedHosts": "*",
  "Features": {
    "TeamCart": true
  },
  "TeamCartStore": {
    "KeyPrefix": "yz:prod",
    "TtlMinutes": 60,
    "UpdatesChannel": "teamcart:updates",
    "MaxItemsPerCart": 200
  },
  "RateLimiting": {
    "OtpRequest": {
      "PerIp": {
        "PerMinute": 5,
        "PerHour": 30
      },
      "PerPhone": {
        "PerMinute": 1,
        "PerHour": 10
      }
    },
    "OtpVerify": {
      "PerIp": {
        "Per5Min": 10
      },
      "PerPhone": {
        "FailedAttemptsWindowMinutes": 10,
        "LockoutMinutes": 5,
        "MaxFailedAttempts": 5
      }
    }
  }
}
