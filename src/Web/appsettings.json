{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AZURE_KEY_VAULT_ENDPOINT": "",
  "AllowedHosts": "*",
  "Features": {
    "TeamCart": true
  },
  "TeamCartStore": {
    "KeyPrefix": "yz:prod",
    "TtlMinutes": 60,
    "UpdatesChannel": "teamcart:updates",
    "MaxItemsPerCart": 200
  },
  "Seeding": {
    "Profile": "Minimal",
    "EnableIdempotentSeeding": true,
    "ClearExistingData": false,
    "SeedTestData": false,
    "EnabledSeeders": {
      "Role": true,
      "User": false,
      "Tag": true,
      "RestaurantBundle": false
    },
    "SeederSettings": {
      "RestaurantBundle": {
        "ReportOnly": true,
        "UpdateDescriptions": false,
        "UpdateBasePrices": false,
        "RestaurantGlobs": ["*.restaurant.json"]
      }
    }
  },
  "RateLimiting": {
    "OtpRequest": {
      "PerIp": {
        "PerMinute": 5,
        "PerHour": 30
      },
      "PerPhone": {
        "PerMinute": 1,
        "PerHour": 10
      }
    },
    "OtpVerify": {
      "PerIp": {
        "Per5Min": 10
      },
      "PerPhone": {
        "FailedAttemptsWindowMinutes": 10,
        "LockoutMinutes": 5,
        "MaxFailedAttempts": 5
      }
    }
  }
  ,
  "ImageProxy": {
    "AllowedHosts": [],
    "RequireSignatureForNonAllowlisted": true,
    "BlockPrivateNetworks": true,
    "AllowHttp": false,
    "MaxBytes": 5242880,
    "TimeoutSeconds": 10,
    "ConnectTimeoutSeconds": 5,
    "CacheSeconds": 86400,
    "MaxRedirects": 3,
    "AllowedContentTypes": [
      "image/jpeg",
      "image/png",
      "image/webp",
      "image/gif"
    ],
    "UserAgent": "YummyZoom-ImageProxy/1.0",
    "EnableSignature": false
  }
}
